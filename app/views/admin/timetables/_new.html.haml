= form_for [:admin, @university, @faculty, @specialty, @group, @timetable] do |s|
	= hidden_field_tag :group_id, @group.id
	= s.select(:current_timetable, ["Current timetable for #{@group.title}"], {}, {:class => "form-control form-for-timetable"})
	= s.submit :class => "btn btn-default add-btn-default"